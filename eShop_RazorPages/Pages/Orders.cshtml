@page
@model OrdersModel
@{
    ViewData["Title"] = "Orders";
}

<h2>Orders</h2>

<table class="table">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Order Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model.Orders)
        {
            <tr>
                <td>@order.OrderId</td>
                <td>@order.OrderDate</td>
            </tr>
        }
    </tbody>
</table>

<a class="btn btn-secondary mt-3" href="Account">Back to Account</a>
